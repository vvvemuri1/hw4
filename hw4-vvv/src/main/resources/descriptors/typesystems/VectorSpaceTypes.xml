<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>VectorSpaceTypes</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Document</name>
      <description/>
      <supertypeName>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.BaseAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokenList</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
          <elementType>edu.cmu.vector_space.typesystems.Token</elementType>
        </featureDescription>
      <featureDescription>
          <name>relevanceValue</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>queryId</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>bigramList</name>
          <description/>
          <rangeTypeName>uima.cas.FSList</rangeTypeName>
        <elementType>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Bigram</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Token</name>
      <description/>
      <supertypeName>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.BaseAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>frequency</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>queryId</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.BaseAnnotation</name>
      <description>Common core attributes shared by all types.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>componentId of creator.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>Confidence of the creator in the correctness of the output on a scale of 0 to 1.0.</description>
          <rangeTypeName>uima.cas.Float</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Bigram</name>
      <description>A bigram type containing two consecutive tokens</description>
      <supertypeName>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.BaseAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>firstToken</name>
          <description>First token of the bigram</description>
          <rangeTypeName>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Token</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>secondToken</name>
          <description>The second token of the bigram</description>
          <rangeTypeName>edu.cmu.lti.f13.hw4.hw4_vvv.typesystems.Token</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
